{
  "name": "dmind/bucket-pagination",
  "description": "Bucket Pagination for TYPO3",
  "license": "GPL-3.0-or-later",
  "type": "typo3-cms-extension",
  "keywords": [
    "TYPO3",
    "pagination"
  ],
  "authors": [
    {
      "name": "Steffen Keuper",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/dmind-gmbh/extension-bucket-pagination",
  "require": {
    "typo3/cms-core": ">=10.4.26"
  },
  "replace": {
    "typo3-ter/bucket-pagination": "self.version"
  },
  "suggest": {
    "georgringer/numbered-pagination": ">=1.0.0"
  },
  "autoload": {
    "psr-4": {
      "Dmind\\BucketPagination\\": "Classes"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "bucket_pagination"
    }
  }
}
